<table summary="Upcoming Event Dates" class="tblStyle fillWidth">
    <thead>
        <tr>
            <th>Group</th>
            <th>Members</th>
        </tr>
    </thead>
    <tbody>
        {% set i=0 %}
        {% for group in groups %}
            {% set i=i+1 %}
        <tr {% if i%2 %}class="fill"{% endif %}>
            <td><a href="{{ path('group_show', {'slug': group.slug}) }}">{{ group.group_name }}</a></td>
            <td>{{ group.member_count }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>