{% form_theme form theme %}

<h1>{{ 'platformd.user.reset.title' | trans }}</h1>
sup
<form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_reset">
    {{ form_widget(form) }}
    <div>
        <input type="submit" class="button btn btn-primary" value="{{ 'resetting.reset.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
    {{ form_rest(form) }}
</form>
