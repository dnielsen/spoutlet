<div class="socialLogin" data-url="{{ path('twitter_login') }}">
    <div type="button" class="rounded Button hasText Module large ajax btn unAuthTwitterConnect registerLoginButton">
        <em></em>
        <span class="buttonText">Login with Twitter</span>
    </div>
</div>
