{% extends 'SpoutletBundle::layout.html.twig' %}

{% block title 'Galleries' %}

{% block content %}

<h3>ALIENWARE ARENA PHOTOS</h3>
<div class="std_2col">
    <div class="left">
        <div style="width: 636px; height: 295px;background-color: #4e4e4e">
            <div class="slider-wrapper theme-nivo">
                <div id="slider" class="nivoSlider">
                    <img src="http://img181.poco.cn/mypoco/myphoto/20110419/20/5596465420110419204210015.jpg" alt="" />
                    <img src="http://i.dailymail.co.uk/i/pix/2009/09/21/article-1215067-0687982F000005DC-42_634x295.jpg" alt="" />
                    <img src="http://tech.mthai.com/wp-content/uploads/2011/11/article-2067262-0EF832D000000578-41_634x312.jpg" alt="" />
                    <img src="http://img181.poco.cn/mypoco/myphoto/20110419/20/5596465420110419204210015.jpg" alt="" />
                    <img src="http://reefbuilders.com/files/2010/10/paul.jpg" alt="" />
                </div>
            </div>
        </div>
    </div>
    <div class="right">
        <div class="game-feed widget-33">
            <div class="widget-title">News</div>
            <div class="widget-content normalized">
            </div>
        </div>
    </div>
</div>
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/spoutlet/js/nivo-slider/nivo-slider.css') }}" type="text/css" media="screen" />
    <link rel="stylesheet" href="{{ asset('bundles/spoutlet/js/nivo-slider/themes/nivo/nivo.css') }}" type="text/css" media="screen" />
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/spoutlet/js/nivo-slider/jquery.nivo.slider.pack.js') }}" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {
            $('#slider').nivoSlider();
        });
    </script>
{% endblock %}
