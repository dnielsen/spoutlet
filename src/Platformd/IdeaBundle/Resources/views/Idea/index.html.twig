{% extends 'IdeaBundle::event_layout.html.twig' %}{% block content %}    <div class="row1">        <div class="cBx1">        <!-- Commenting out slider code for now            <!-- slider code start            <div class="intro">                <div id="slideshow" class="pics">                    <div class="unicusSlides">                        <a href="/" target="_blank">                            <img src={{ asset('bundles/idea/img/SVIC_banner_color.jpg') }} alt="" width="644" height="340">                        </a>                    </div>                    <div class="unicusSlides">                        <a href="/" target="_blank">                            <img src={{ asset('bundles/idea/img/slide2.jpg') }} alt="" width="644" height="340">                        </a>                    </div>                    <div class="unicusSlides">                        <a href="/" target="_blank">                            <img src={{ asset('bundles/idea/img/slide3.jpg') }} alt="" width="644" height="340">                        </a>                    </div>                    <div class="unicusSlides">                        <a href="/" target="_blank">                            <img src={{ asset('bundles/idea/img/slide4.jpg') }} alt="" width="644" height="340">                        </a>                    </div>                    <div class="unicusSlides">                        <a href="/" target="_blank">                            <img src={{ asset('bundles/idea/img/slide5.jpg') }} alt="" width="644" height="340">                        </a>                    </div>                    <div class="unicusSlides">                        <a href="/" target="_blank">                            <img src={{ asset('bundles/idea/img/slide6.jpg') }} alt="" width="644" height="340">                        </a>                    </div>                </div><!-- end jQuery Cycle                <ul id="bullets"></ul> <!-- nav bullets             </div>            <script type="text/javascript">                $(function() {                    $('#slideshow').cycle({                        fx:     'scrollHorz',                        speed:  400,                        timeout: 6000,                        cleartypeNoBg: true,                        pager:  '#bullets',                        pagerAnchorBuilder: function paginate(idx, el) {                            return '<a class="service' + idx + '" href="#" >&bull;</a>';                        }                    });                });            </script>        Remember to fix all the html endcomment tags when you remove this one -->            <p>{{event.content | nl2br }}</p>        </div>        <div class="cBx2">            <a href="{{ path('idea_show_all', {'groupSlug': groupSlug, 'eventSlug': eventSlug}) }}" class="submit left" style="margin-left: 10%">View Ideas</a>            {% if app.user %}                <a href="{{ path('idea_create_form', {'groupSlug': groupSlug, 'eventSlug': eventSlug}) }}" class="submit right" style="margin-right: 10%">Add Idea</a>            {% else %}                <a href="{{ path('fos_user_registration_register') }}" class="submit right" style="margin-right: 10%">Join Now</a>            {% endif %}            <table id="eventDetails" class="tblStyle">                <tbody>                    <tr>                        <td width="25%"><strong>Event:</strong></td>                        <td width="75%">{{event.name}}</td>                    </tr>                    <tr>                        <td width="25%"><strong>Date:</strong></td>                        <td width="75%">{{event.DateRangeString}}</td>                    </tr>                    <tr>                        <td width="25%"><strong>Location:</strong></td>                        <td width="75%">{{ event.location }}</td>                    </tr>                    <tr>                        <td width="25%"><strong>Address:</strong></td>                        <td width="75%">{{ event.address1 }}<br/>{{ event.address2 }}</td>                    </tr>                </tbody>            </table>            <br><!--            <iframe width="364" height="170" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?f=q&amp;source=s_q&amp;hl=en&amp;geocode=&amp;q=San+Jos%C3%A9+State+University,+San+Jose,+CA&amp;aq=0&amp;oq=San+Jose+State+Un&amp;sll=37.6,-95.665&amp;sspn=56.021494,135.263672&amp;ie=UTF8&amp;hq=San+Jos%C3%A9+State+University,+San+Jose,+CA&amp;t=m&amp;ll=37.335826,-121.8812&amp;spn=0.008616,0.009539&amp;z=12&amp;output=embed"></iframe><br>            <a href="https://maps.google.com/maps?f=q&amp;source=embed&amp;hl=en&amp;geocode=&amp;q=San+Jos%C3%A9+State+University,+San+Jose,+CA&amp;aq=0&amp;oq=San+Jose+State+Un&amp;sll=37.6,-95.665&amp;sspn=56.021494,135.263672&amp;ie=UTF8&amp;hq=San+Jos%C3%A9+State+University,+San+Jose,+CA&amp;t=m&amp;ll=37.335826,-121.8812&amp;spn=0.008616,0.009539" class="smtxt right">View Larger Map &rsaquo;</a>-->        </div>    </div>{% endblock %}